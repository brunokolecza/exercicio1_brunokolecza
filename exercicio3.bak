EQUATION("X") // vendas da firma
v[0] = V("a");
v[1] = V("P");
v[2] = V("b");
v[3] = V("Q");
RESULT(v[0] * v[1] + v[2] * v[3] + uniform(-1,1)) 

EQUATION("X_sum")
RESULT(SUM("X"))

EQUATION("X_share") // parcela de mercado da firma
v[0] = V("X");
v[1] = V("X_sum");
if(v[1] != 0)
v[2] = v[0]/v[1];
else
v[2] = 0;
RESULT(v[2])

EQUATION("P") // preco do produto
v[0] = V("X_share") - VL("X_share", 1);
v[1] = V("c");
if (v[0] > 0)
v[2] = VL("P", 1) + v[1] * v[0];
else
v[2] = VL("P", 1) - v[1] * v[0];
RESULT(v[2])

 
EQUATION("Q") // qualidade do produto
v[0] = VL("Q",1) + uniform(-1, 1);
v[1] = VL("Q", 1);
if (v[0] > v[1])
v[2] = v[0];
else
v[2] = v[1];
RESULT(v[2])













